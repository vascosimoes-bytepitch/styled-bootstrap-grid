{
  "name": "styled-bootstrap-grid",
  "version": "3.1.2",
  "description": "bootstrap grid system using styled components",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "rm -rf dist/ && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "prepublishOnly": "npm run build",
    "postinstall": "npm run build",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vascosimoes-bytepitch/styled-bootstrap-grid.git"
  },
  "keywords": [
    "styled-components",
    "bootstrap",
    "react-bootstrap",
    "layout",
    "grid",
    "grid system",
    "styled grid",
    "styled layout",
    "styled bootstrap grid"
  ],
  "author": "Florent BÃ©jina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vascosimoes-bytepitch/styled-bootstrap-grid/issues"
  },
  "homepage": "https://github.com/vascosimoes-bytepitch/styled-bootstrap-grid#readme",
  "peerDependencies": {
    "react": "^0.14.0 || >=16",
    "styled-components": ">=4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/styled-components": "^4.1.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.15.3",
    "typescript": "^3.2.1"
  }
}
